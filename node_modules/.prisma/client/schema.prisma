generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Subscription {
  id       Int      @id @default(autoincrement())
  name     String
  email    String   @unique
  createAt DateTime @default(now())
}
